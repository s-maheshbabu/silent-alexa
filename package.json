{
  "name": "silent_alexa",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://s-maheshbabu.github.io/silent-alexa",
  "dependencies": {
    "gh-pages": "^1.1.0",
    "http-message-parser": "0.0.30",
    "immutable": "^4.0.0-rc.9",
    "material-ui": "^0.20.0",
    "monkas-chat": "^1.0.11",
    "npm": "^5.8.0",
    "react": "^16.2.0",
    "react-container-dimensions": "^1.3.3",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "query-string": "^5.1.1",
    "react-scripts": "1.1.4",
    "uuid": "^3.2.1"
  },
  "config": {
    "nodepath": "src:src/components:src/avs:src/auth:src/constants"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "NODE_PATH=$npm_package_config_nodepath react-scripts start",
