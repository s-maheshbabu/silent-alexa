{
  "name": "silent_alexa",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://s-maheshbabu.github.io/silent-alexa",
  "dependencies": {
    "gh-pages": "^1.1.0",
    "http-message-parser": "0.0.30",
    "immutable": "^4.0.0-rc.9",
    "js-cookie": "^2.2.0",
    "material-ui": "^0.20.0",
    "monkas-chat": "^1.0.11",
    "npm": "^6.13.4",
    "query-string": "^5.1.1",
    "react": "^16.8.6",
    "react-container-dimensions": "^1.3.3",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.0.1",
    "uuid": "^3.2.1"
  },
  "config": {
    "nodepath": "src:src/components:src/avs:src/auth:src/constants"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "NODE_PATH=$npm_package_config_nodepath react-scripts start",
    "build": "NODE_PATH=$npm_package_config_nodepath react-scripts build",
    "test": "NODE_PATH=$npm_package_config_nodepath react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.1",
    "clone": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "fetch-mock": "^6.1.0",
    "react-addons-test-utils": "^15.6.2",
    "sprintf-js": "^1.1.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
