    "test": "NODE_PATH=$npm_package_config_nodepath react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "clone": "^2.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "fetch-mock": "^6.1.0",
    "react-addons-test-utils": "^15.6.2",
    "sprintf-js": "^1.1.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
